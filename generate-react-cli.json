{
  "usesTypeScript": false,
  "usesCssModule": true,
  "cssPreprocessor": "scss",
  "testLibrary": "Testing Library",
  "component": {
    "default": {
      "path": "src/App/components",
      "withStyle": true,
      "withTest": true,
      "withStory": false,
      "withLazy": false
    },
    "hookedFunc": {
      "path": "src/App/components",
      "withStyle": true,
      "withTest": true,
      "withStory": false,
      "withLazy": false,
      "customTemplates": {
        "component": "grc_templates/funcWithState/TemplateName.js"
      }
    },
    "layoutFunc": {
      "path": "src/App/components/layouts",
      "withStyle": true,
      "withTest": true,
      "withStory": false,
      "withLazy": false,
      "customTemplates": {
        "component": "grc_templates/funcLayout/TemplateName.js"
      }
    }
  }
}
